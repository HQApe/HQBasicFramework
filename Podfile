platform:ios,'8.0'

# 忽略引入库的所有警告（强迫症者的福音啊）
inhibit_all_warnings!

target 'HQBasicFrame' do 

# 启动广告
pod 'XHLaunchAd'
# 特性引导页
pod 'Onboard'
# 导航控制器
pod 'RTRootNavigationController'
# 空布局
pod 'DZNEmptyDataSet'
# 提示
pod 'Toast', '~> 4.0.0'
#网络请求
pod 'AFNetworking'
#Bugly奔溃记日志处理，使用腾讯后台
pod 'Bugly'
#用户行为统计
pod 'QQ_MTA'

#奔溃日志
pod 'KSCrash', '~> 1.15.16', :configurations => ['Debug']
#非奔溃日志
pod 'CocoaLumberjack', :configurations => ['Debug']

end

